@page
@model ProjetASI.Pages.Tables.CreateModel
@{
    ViewData["Title"] = "Create Article";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Article.nom">Nom de l'article'</label>
        <input asp-for="Article.nom" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Article.prixU">Prix Unitaire</label>
        <input asp-for="Article.prixU" class="form-control" rows="10"></input>
    </div>
    <div class="form-group">
        <label asp-for="Article.quantite"></label>
        <input asp-for="Article.quantite" type="int" class="form-control" rows="10"></input>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
 